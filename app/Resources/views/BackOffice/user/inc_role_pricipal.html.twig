{# empty Twig template #}
    {% if (user.isGestionnaire) %}
        {% set user_type='ROLE_GESTIONNAIRE' %}
    {% elseif (user.IsAnEtablissement) %}
        {% set user_type='ROLE_USER' %}
    {% else %}
        {% set user_type='SUPER_ADMIN' %}
     {% endif %}
     
<input id="user_username" name="user_type" required="required" class="form-control" value="{{user_type}}" type="text">
