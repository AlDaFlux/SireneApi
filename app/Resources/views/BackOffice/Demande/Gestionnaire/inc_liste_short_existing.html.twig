{# empty Twig template #}
    <table class='table table-bordered table-triable_simple_desc small'>
        <thead>
            <tr>
                <th>Date</th>
                <th>Gestionnaire</th>

                {% if  is_granted('ROLE_SUPER_ADMIN_TRAITEMENT_DEMANDE') %}                    
                    <th>Demande</th>
                    <th>Creai</th>
                    
                {% endif %}
                <th>Etabs</th>
            </tr>
        </thead>
        <tbody>
        {% for gestionnaire in gestionnaires %}
                <tr>
                <td>
                    
                    
                        {{ gestionnaire.getDemandesEtablissementGestionnaireExistantNonFiniLastDate|hiddenDate("d-m-Y") | raw }} 
                </td>

                {% if  is_granted('ROLE_SUPER_ADMIN_TRAITEMENT_DEMANDE') %}
                    <td>
                             <a title='voir le gestionnaire' href="{{ path('backoffice_gestionnaire_show', { 'id': gestionnaire .id }) }}">{{ gestionnaire}} </a>
                    </td>
                    <td>
                             <a title='voir les demandes en cours pour le gesitionnaire' href="{{ path('demande_gestionnaire_existant_id', { 'id': gestionnaire .id }) }}">{{ 'demande' | get_icon | raw }}</a>
                    </td>
                    <td>{{ gestionnaire.creai  }}</td>
                {% else  %}
                    <td>
                             <a title='voir les demandes en cours pour le gesitionnaire' href="{{ path('demande_gestionnaire_existant_id', { 'id': gestionnaire .id }) }}">{{ gestionnaire}} </a>
                    </td>
                {% endif  %}
                
                <td>
                        {{ gestionnaire.getNbDemandesEtablissementGestionnaireExistantNonFini}} 
                </td>
                
            </tr>
        {% endfor %}
        </tbody>
    </table>