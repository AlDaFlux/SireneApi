{# empty Twig template #}
    <table class='table table-bordered table-triable_simple_desc small'>
        <thead>
            <tr>
                <th>Date</th>
                <th>Gestionnaire</th>

                {% if  is_granted('ROLE_SUPER_ADMIN_TRAITEMENT_DEMANDE') %}                    
                    <th>Creai</th>
                {% endif %}
                <th>Etat</th>

                
                
            </tr>
        </thead>
        <tbody>
        {% for demandeGestionnaire in demandesGestionnaire %}
            {% if demandeGestionnaire.etat.id != 3 %}
                <tr title="la demande n'est pas terminée mais l'établissement est créer" class='alert-danger'>
            {% else %}
                <tr>
            {% endif  %}
                <td>{{ demandeGestionnaire.dateDemande|hiddenDate("d-m-Y") | raw }}</td>
                <td><a href="{{ path('demande_gestionnaire_show', { 'id': demandeGestionnaire.id }) }}"> {{ demandeGestionnaire.GestionnaireNom }} </td>
                {% if  not is_granted('ROLE_SUPER_ADMIN_TRAITEMENT_DEMANDE') %}
                    <td>{{ demandeGestionnaire.creai  }}</td>
                {% endif %}
                <td>{{ demandeGestionnaire.etat|  afficheDemandeEtat | raw }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>