{% for objectif  in objectifs %}

    <h2> {{ objectif }}
        <table class='table-bordered table-pourcent' width="100%">
            <tr>
                 
                {% if objectif.CompleteTrue %}
                        <td width='100%' class='effectue'></td>
                {% elseif  objectif.NonCommence %}
                        <td width='100%' class='noncommence'></td>
                {% else  %}
                    <td width='{{objectif.complete}}%' class='effectue'></td>
                    <td width='{{objectif.unComplete}}%' class='afaire'></td>
                {% endif  %}
            </tr>
        </table> 
        </h2>
    
            
        <table class='table-bordered' width="100%">
        <tr>
            <td>Objectif {{ objectif.CompleteLib }}  {{ objectif.complete | completeCss | raw  }}  </td>
            <td class='tmpdt'>Début </td>
            <td class='tmpdt'>{{ objectif.dateDebut is empty ? "" : objectif.dateDebut|date("d-m-Y") }}</td>
        
            <td class='tmpdt'>Fin </td>
            <td class='tmpdt'>{{ objectif.dateFin is empty ? "" : objectif.dateFin|date("d-m-Y") }}</td>
        </tr>
        </table> 

        
             
                        
                        
                        {% if objectif.Description %}
                            <div><b>Description : </b>{{ objectif.Description }}</div>
                        {% endif %}
                        {% if objectif.Moyen %}
                            <div><b>Moyens : </b>{{ objectif.Moyen }}</div>
                        {% endif %}
                        {% if objectif.indicateurs %} 
                            <div><b>Indicateurs : </b>{{ objectif.indicateurs }}</div>
                        {% endif %}
                        
                        {% if objectif.NbPreuves %}
                            <div><b>Preuves : </b>
                            {% include "Synthese/export/inc/preuves/inc_preuves_link.html.twig" with {'preuves':objectif.Preuves } %}
                            </div>
                        {% endif %}

                        {% if objectif.criteres %}
                            <div><b>Critères: </b>
                                <ul>
                            {% for critere  in objectif.criteres %}
                                <li>{{critere.numero}} : {{critere}}
                            {% endfor %}
                                </ul>
                            </div>
                        {% endif %}
    
                   
{% endfor %}
