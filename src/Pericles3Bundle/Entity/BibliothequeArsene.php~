<?php

namespace Pericles3Bundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Bibliotheque
 *
 * @ORM\Table(name="bibliotheque_arsene")
 * @ORM\Entity(repositoryClass="Pericles3Bundle\Repository\BibliothequeArseneRepository")
 */
class BibliothequeArsene
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

 
    /**
     * @var string
     *
     * @ORM\Column(name="message", type="string", length=255)
     */
    private $titre;
     
                  
         
    /**
     * @ORM\ManyToMany(targetEntity="Pericles3Bundle\Entity\ReferentielPublic", inversedBy="BibliothequeArsene")
     */
    private $ReferentielPublic;
    
    
    /**
     * @var string
     *
     * @ORM\Column(name="href", type="string", length=255)
     */
    private $href;

  
    
    
    /**
    * @ORM\ManyToOne(targetEntity="Pericles3Bundle\Entity\BibliothequeAncreaiTypeSource", inversedBy="bibliothequesAncreai")
    * @ORM\JoinColumn(nullable=false)
    */
    private $typeSourceBA;




    
    /**
     * @var \DateTime
     *
     * @ORM\Column(name="date_update", type="datetime")
     */
    private $dateUpdate;

   
    
    
    /**
     * toString
     * @return string
     */
    public function __toString() 
    {
        return $this->getTitre();
    }
    /**
     * Constructor
     */
    public function __construct()
    {
        $this->ReferentielPublic = new \Doctrine\Common\Collections\ArrayCollection();
    }

    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set titre
     *
     * @param string $titre
     *
     * @return BibliothequeArsene
     */
    public function setTitre($titre)
    {
        $this->titre = $titre;

        return $this;
    }

    /**
     * Get titre
     *
     * @return string
     */
    public function getTitre()
    {
        return $this->titre;
    }

    /**
     * Add referentielPublic
     *
     * @param \Pericles3Bundle\Entity\ReferentielPublic $referentielPublic
     *
     * @return BibliothequeArsene
     */
    public function addReferentielPublic(\Pericles3Bundle\Entity\ReferentielPublic $referentielPublic)
    {
        $this->ReferentielPublic[] = $referentielPublic;

        return $this;
    }

    /**
     * Remove referentielPublic
     *
     * @param \Pericles3Bundle\Entity\ReferentielPublic $referentielPublic
     */
    public function removeReferentielPublic(\Pericles3Bundle\Entity\ReferentielPublic $referentielPublic)
    {
        $this->ReferentielPublic->removeElement($referentielPublic);
    }

    /**
     * Get referentielPublic
     *
     * @return \Doctrine\Common\Collections\Collection
     */
    public function getReferentielPublic()
    {
        return $this->ReferentielPublic;
    }

    /**
     * Set href
     *
     * @param string $href
     *
     * @return BibliothequeArsene
     */
    public function setHref($href)
    {
        $this->href = $href;

        return $this;
    }

    /**
     * Get href
     *
     * @return string
     */
    public function getHref()
    {
        return $this->href;
    }

    /**
     * Set dateUpdate
     *
     * @param \DateTime $dateUpdate
     *
     * @return BibliothequeArsene
     */
    public function setDateUpdate($dateUpdate)
    {
        $this->dateUpdate = $dateUpdate;

        return $this;
    }

    /**
     * Get dateUpdate
     *
     * @return \DateTime
     */
    public function getDateUpdate()
    {
        return $this->dateUpdate;
    }

    /**
     * Set typeSourceBA
     *
     * @param \Pericles3Bundle\Entity\BibliothequeAncreaiTypeSource $typeSourceBA
     *
     * @return BibliothequeArsene
     */
    public function setTypeSourceBA(\Pericles3Bundle\Entity\BibliothequeAncreaiTypeSource $typeSourceBA)
    {
        $this->typeSourceBA = $typeSourceBA;

        return $this;
    }

    /**
     * Get typeSourceBA
     *
     * @return \Pericles3Bundle\Entity\BibliothequeAncreaiTypeSource
     */
    public function getTypeSourceBA()
    {
        return $this->typeSourceBA;
    }
}
